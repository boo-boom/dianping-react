{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["react-app"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "linebreak-style": ["error", "unix"],
    "semi": ["error", "always"],
    "no-console": "off",
    "quotes": ["error", "double"],
    "react/prefer-stateless-function": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "react/forbid-prop-types": 0
  }
}
